{
    "name":"Take a Break",
    "version":"1.0.7",
    "manifest_version":2,
    "description":"We remind you to take a Break ",
    "icons": {
        "512": "code.png"
      },
    "browser_action":{
        "default_icon": "code.png",
        "default_title":"Take a Break",
        "default_popup":"index.html"
    },
    "permissions": ["alarms", "notifications"],
    "background":{
        "scripts":["background.js"]
    }
}